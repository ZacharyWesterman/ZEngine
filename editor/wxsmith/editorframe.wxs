							</object>
							<flag>wxALL|wxEXPAND</flag>
							<border>5</border>
							<option>3</option>
						</object>
					</object>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<option>1</option>
			</object>
		</object>
